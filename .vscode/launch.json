{
  "version": "0.2.0",
  "configurations": [
    
    {
      "type": "node",
      "request": "launch",
      "name": "Next.js: Debug API",
      "runtimeExecutable": "node",
      "runtimeArgs": ["--inspect-brk", "./node_modules/next/dist/bin/next", "dev"],
      "port": 9229,
      "cwd": "${workspaceFolder}",
      "protocol": "inspector",
      "console": "integratedTerminal"
    }
  ]
}
